{

    /* manifest header */
  "uberspark-manifest":{
      "manifest_node_types" : [ "uberspark-uobjcoll" ],
      "uberspark_min_version" : "5.1",
      "uberspark_max_version" : "any"
  },

  /* Ã¼objcoll declarations */
  "uberspark-uobjcoll":{
      "namespace" : "uberspark/uobjcoll/platform/pc/uxmhf",
      "platform" : "pc",
      "arch" : "x86_32",
      "cpu" : "generic",
      "hpl" : "any", //"hyp",
      "sentinels-intrauobjcoll" : [ "generic/generic/any/call" ],

      /* uobjs */
      "uobjs" : {
          "master" : "",
          "templars" : [
              "uberspark/uobjcoll/platform/pc/uxmhf/main"
          ]
      },


    /* uobjcoll init method */
    "initmethod" : {
        "uobj_ns" : "uberspark/uobjcoll/platform/pc/uxmhf/main",
        "pm_name" : "main",
        "sentinels" : [
            {
                "sentinel_type" : "baremetal/grub-legacy/srot/generic/pc/pmr0/call"
            }	
        ]
    },	


      /* uobjcoll public methods */
      "publicmethods" : {
          "uberspark/uobjcoll/platform/pc/uxmhf/main" : {
              "main" : [ "generic/generic/any/call" ]
          }
      }

  }

}
